{
    "name": "pfaffkit/essa",
    "version": "1.0.0",
    "description": "Event Sourcing Scaffold Assembly",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Pfaff",
            "email": "kamil@pfaff.app"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/uid": "^7.2",
        "symfony/http-kernel": "7.2",
        "symfony/dependency-injection": "7.2",
        "symfony/config": "7.2",
        "ext-pdo": "*",
        "symfony/serializer": "7.2",
        "symfony/property-info": "7.2",
        "symfony/property-access": "7.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/yaml": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "friendsofphp/php-cs-fixer": "^3.68",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PfaffKIT\\Essa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PfaffKIT\\Essa\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "pfaffkit/essa-storage-doctrine-connector": "Doctrine persistence connector."
    }
}
